class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        // code here
        
        //creating an array list
        ArrayList<Integer>list=new ArrayList<>();
        
        for(int i=0;i<n;i++)
        {
            int index=i+1;
            if(arr[i]==index)
            {
                list.add(index);
            }
        }
        return list;
    }
}
