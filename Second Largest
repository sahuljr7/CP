//User function Template for Java

class Solution {
   int print2largest(int arr[], int n) {
    // code here
   int max1=0;
   int max=0;
    
    //This loop checks the largest element
    for(int i=0;i<n;i++)
    {
        if(arr[i]>max)
        max=arr[i];
        
    }
    
    //This loop check the second largest element
    for(int i=0;i<n;i++)
    {
        if(arr[i]>max1 && arr[i]!=max)
        max1=arr[i];
    }
    
    
  if(max1==0)
  return -1;
  
return max1;
}
}
